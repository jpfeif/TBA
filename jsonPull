import requests


event = "2019wimi"

eventTeams = "https://www.thebluealliance.com/api/v3/event/" + event + "/teams"

teams = requests.get(eventTeams, params={'X-TBA-Auth-Key': 'oJ8q81FBEVKJlR3rVaVKhgJ3nGxjeGNy44DktYMHxdtBRjTPLBI2hIZX95SRNxXk'})

data = teams.json()

for x in data:
    print (x['name'])
    print (x['address'])
